@model TrabajoFinal.ViewModels.Home.DashboardProveedorViewModel
@{
    ViewBag.Title = "Dashboard Proveedor";
    ViewBag.PageIcon = "tachometer";
}

<div class="col-md-6 col-lg-3">
    <div class="widget-bg-color-icon card-box fadeInDown animated">
        <div class="bg-icon bg-icon-info pull-left">
            <i class="fa fa-gavel"></i>
        </div>
        <div class="text-right">
            <h3 class="text-dark"><b class="counter">@Model.CantSupervisores</b></h3>
            <p class="text-muted">Supervisores</p>
        </div>
        <div class="clearfix"></div>
    </div>
</div>
<div class="col-md-6 col-lg-3">
    <div class="widget-bg-color-icon card-box fadeInDown animated">
        <div class="bg-icon bg-icon-custom pull-left">
            <i class="fa fa-user"></i>
        </div>
        <div class="text-right">
            <h3 class="text-dark"><b class="counter">@Model.CantEmpleados</b></h3>
            <p class="text-muted">Empleados</p>
        </div>
        <div class="clearfix"></div>
    </div>
</div>